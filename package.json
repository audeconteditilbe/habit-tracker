{
  "name": "habit-tracker",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:rest-types": "TS_NODE_PROJECT=./tsconfig.scripts.json node --loader=ts-node/esm scripts/build-rest-types.ts",
    "build:code": "run-p type-check \"build-only {@}\" --",
    "build": "run-p 'build:rest-types' 'build:code'",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "@apollo/client": "^3.12.4",
    "axios": "^1.7.9",
    "graphql": "^16.10.0",
    "jwt-decode": "^4.0.0",
    "openapi-fetch": "^0.13.3",
    "pinia": "^2.3.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.1",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/eslint-plugin": "1.1.20",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.14.0",
    "eslint-plugin-playwright": "^2.1.0",
    "eslint-plugin-vue": "^9.30.0",
    "jsdom": "^25.0.1",
    "json-schema-to-typescript": "^15.0.3",
    "json-server": "^1.0.0-beta.3",
    "npm-run-all2": "^7.0.2",
    "openapi-typescript": "^7.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vite": "^6.0.5",
    "vite-plugin-vue-devtools": "^7.6.8",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.1.10",
    "yaml": "^2.7.0"
  },
  "packageManager": "yarn@4.5.3"
}
